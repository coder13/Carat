{
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "quotes": [
            0,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            1,
            "always"
        ],
        "dot-notation": 0,
        "comma-dangle": 0,
        "curly": 0,
        "no-console": 0,
        "consistent-return": 0,
        "no-use-before-define": 0,
        "strict": 0,
        "no-eval": 0,
        "eqeqeq": 0
    }
    "ecmaFeatures": {
        "arrowFunctions": true,
        "blockBindings": true,
        "objectLiteralComputedProperties": true,
        "objectLiteralShorthandMethods": true,
        "objectLiteralShorthandProperties": true,
        "templateStrings": true,
    },
    "env": {
        "node": true
    }
}